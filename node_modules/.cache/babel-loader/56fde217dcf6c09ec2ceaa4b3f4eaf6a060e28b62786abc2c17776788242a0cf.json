{"ast":null,"code":"import { defineComponent } from \"vue\";\nimport NavBar from \"../components/NavBar.vue\";\nimport FooterView from \"../components/FooterView.vue\";\nimport axios from 'axios';\nexport default defineComponent({\n  name: \"HomeView\",\n  components: {\n    NavBar,\n    FooterView\n  },\n  data() {\n    return {\n      products: []\n    };\n  },\n  mounted() {\n    let something = axios.get('https://63e68b1a7eef5b2233882583.mockapi.io/events/events').then(response => this.products = response.data);\n    console.log(something);\n  },\n  methods: {\n    addProductToCart(event) {\n      this.$store.dispatch('addProductToCart', event);\n    },\n    added(number) {\n      let btn = document.getElementById(\"buttonBuyNow\" + number);\n      btn.innerHTML = \"ADDED TO CART\";\n      btn.style.backgroundColor = \"green\";\n      setTimeout(function () {\n        btn.innerHTML = \"BUY NOW\";\n        btn.style.backgroundColor = \"\";\n      }, 1500);\n    }\n  }\n\n  //   computed: {\n  //     events() {\n  //       return this.$store.getters.availableEvents\n  //     }\n  //   },\n});\n//console.log(events);","map":{"version":3,"mappings":"AAkDA,SAASA,eAAc,QAAS,KAAK;AACrC,OAAOC,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,UAAS,MAAO,8BAA8B;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAeH,eAAe,CAAC;EAC7BI,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVJ,MAAM;IACNC;EACF,CAAC;EACDI,IAAG,GAAK;IACN,OAAO;MACLC,QAAQ,EAAC;IACX;EACF,CAAC;EACDC,OAAM,GAAK;IACT,IAAIC,SAAQ,GAAIN,KAAI,CACjBO,GAAG,CAAC,2DAA2D,EAC/DC,IAAI,CAACC,QAAO,IAAM,IAAI,CAACL,QAAO,GAAIK,QAAQ,CAACN,IAAK;IACjDO,OAAO,CAACC,GAAG,CAACL,SAAS;EACzB,CAAC;EACDM,OAAO,EAAE;IACPC,gBAAgB,CAACC,KAAK,EAAE;MACpB,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,kBAAkB,EAAEF,KAAK;IAClD,CAAC;IACDG,KAAK,CAACC,MAAM,EAAE;MACd,IAAIC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,cAAc,GAACH,MAAM,CAAC;MAExDC,GAAG,CAACG,SAAQ,GAAI,eAAc;MAE9BH,GAAG,CAACI,KAAK,CAACC,eAAc,GAAI,OAAO;MACnCC,UAAU,CAAC,YAAU;QACnBN,GAAG,CAACG,SAAQ,GAAI,SAAQ;QACxBH,GAAG,CAACI,KAAK,CAACC,eAAc,GAAI,EAAE;MAChC,CAAC,EAAC,IA