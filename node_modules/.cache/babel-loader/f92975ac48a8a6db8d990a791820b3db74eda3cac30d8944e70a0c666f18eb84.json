actions>\r\n                <v-btn class=\"text-white\" variant=\"outlined\" :id='\"buttonBuyNowEvent\"+event.id' width=\"150px\" @click=\"addEventToCart(event);addedEvent(event.id)\"> BUY NOW </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n      </v-col>\r\n      </v-row>\r\n      <v-row align=\"center\" justify=\"center\">\r\n        <v-btn class=\"text-white\" variant=\"outlined\" to=\"/events\" style=\"margin-left:10px\"> DISCOVER MORE -> </v-btn>\r\n      </v-row>\r\n    </v-container>\r\n    <v-btn\r\n            v-scroll=\"onScroll\"\r\n            v-show=\"fab\"\r\n            fab\r\n            dark\r\n            icon=\"fa-solid fa-arrow-up\"\r\n            right\r\n            style=\" position:fixed;\r\n                right:10px;\r\n                bottom:10px;\"\r\n            color=\"dark\"\r\n            @click=\"toTop\">\r\n          </v-btn>\r\n    <!-- merch -->\r\n    <v-container fluid style=\"margin-top: 50px\" id=\"shop\">\r\n      <v-row align=\"center\" justify=\"center\">\r\n        <h1 class=\"text-white\">THE MERCH</h1>\r\n      </v-row>\r\n\r\n      <v-row\r\n        align=\"center\"\r\n        justify=\"center\"\r\n        style=\"margin-top: 50px; margin-bottom: 50px\"\r\n       \r\n      >\r\n      <v-col  v-for=\"product in products.slice(0,3)\" :key=\"product.id\" md=\"4\">\r\n              <v-card class=\"mx-auto text-white\" max-width=\"390px\" height=\"\" style=\"padding: 40px;background: black; border:1px solid white;\">\r\n              <v-img src=\"../assets/pictures/white1.jpg\" height=\"200px\" cover></v-img>\r\n              <v-card-title>{{ product.name }} </v-card-title>\r\n              <v-card-text>\r\n                {{ product.color }}\r\n              </v-card-text>\r\n              <v-card-text>\r\n                {{ product.price }} BGN\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-btn class=\"text-white\" variant=\"outlined\" :id='\"buttonBuyNowProduct\"+product.id' width=\"150px\" @click=\"addProductToCart(product);addedProduct(product.id)\"> BUY NOW </v-btn>\r\n                <v-btn class=\"text-white\" variant=\"outlined\" to=\"/whiteShirt\" style=\"margin-left:10px\"> MORE DETAILS </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n      </v-col>\r\n      </v-row>\r\n      <v-row align=\"center\" justify=\"center\">\r\n        <v-btn class=\"text-white\" variant=\"outlined\" to=\"/shop\" style=\"margin-left:10px\"> DISCOVER MORE -> </v-btn>\r\n      </v-row>\r\n\r\n\r\n    </v-container>\r\n\r\n\r\n    <!-- highliths  -->\r\n    <v-container fluid style=\"margin-top: 50px\">\r\n      <v-row align=\"center\" justify=\"center\">\r\n        <h1 class=\"text-white\">HIGHLIGHTS</h1>\r\n      </v-row>\r\n      <v-row\r\n        align=\"center\"\r\n        justify=\"center\"\r\n        style=\"margin-top: 50px; margin-bottom: 200px\"\r\n      >\r\n      <v-row align=\"center\" justify=\"center\" style=\"margin-left:50px; margin-right:50px\">\r\n          <template v-for=\"(item, i) in items\" :key=\"i\">\r\n            <v-col md=\"4\">\r\n              <v-hover v-slot=\"{ isHovering, props }\">\r\n                <v-card\r\n                  :elevation=\"isHovering ? 12 : 2\"\r\n                  :class=\"{ 'on-hover': isHovering }\"\r\n                  v-bind=\"props\"\r\n                >\r\n                  <v-img :src=\"item.img\" height=\"400px\" cover> </v-img>\r\n                </v-card>\r\n              </v-hover>\r\n            </v-col>\r\n          </template>\r\n          </v-row>\r\n      </v-row>\r\n    </v-container>\r\n\r\n\r\n<!-- TEAM   -->\r\n    <v-container fluid style=\"margin-top: 50px\">\r\n      <v-row align=\"center\" justify=\"center\">\r\n        <h1 class=\"text-white\">THE TEAM</h1>\r\n      </v-row>\r\n      <v-row style=\"margin-top: 100px\" align=\"center\" justify=\"center\">\r\n        <v-col>\r\n          <v-divider></v-divider>\r\n          <v-row>\r\n  