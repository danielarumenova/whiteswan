    return this.$store.getters.availableEvents;\n    }\n  },\n  mounted() {\n    this.$store.dispatch('fetchProducts');\n    this.$store.dispatch('fetchEvents');\n    let events = axios.get('https://63e68b1a7eef5b2233882583.mockapi.io/events/events').then(response => this.products = response.data);\n    console.log(events);\n    let products = axios.get('https://63e68b1a7eef5b2233882583.mockapi.io/events/events').then(response => this.products = response.data);\n    console.log(events);\n  }\n});","map":{"version":3,"mappings":"AA+LA,SAASA,eAAc,QAAS,KAAK;;AAErC;AACA,OAAOC,UAAS,MAAO,8BAA8B;AACrD,OAAOC,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAeH,eAAe,CAAC;EAC7BI,IAAI,EAAE,UAAU;EAChBC,KAAK,GAAG;IACN,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,CACL;QACEC,GAAG,EAAEC,OAAO,CAAC,4BAA4B;MAC3C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC,EACD;QACED,GAAG,EAAEC,OAAO,CAAC,2BAA2B;MAC1C,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IAETC,QAAO,CAAGC,CAAC,EAAE;MACT,IAAI,OAAOC,MAAK,KAAM,WAAW,EAAE;MACnC,MAAMC,GAAE,GAAID,MAAM,CAACE,WAAU,IAAOH,CAAC,CAACI,MAAM,CAACC,SAAQ,IAAK;MAC1D,IAAI,CAACC,GAAE,GAAIJ,GAAE,GAAI,EAAC;IACpB,CAAC;IACDK,KAAI,GAAK;MACPN,MAAM,CAACO,QAAQ,CAAC,CAAC,EAAC,CAAC,CAAC;IACtB,CAAC;IACDC,KAAK,CAACC,MAAM,EAAE;MACd,IAAIC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,cAAc,GAACH,MAAM,CAAC;MAExDC,GAAG,CAACG,SAAQ,GAAI,eAAc;MAE9BH,GAAG,CAACI,KAAK,CAACC,eAAc,GAAI,OAAO;MACnCC,UAAU,CAAC,YAAU;QACnBN,GAAG,CAACG,SAAQ,GAAI,SAAQ;QACxBH,GAAG,CAACI,KAAK,CAACC,eAAc,GAAI,EAAE;MAChC,CAAC,EAAC,IAAI;IACR,CAAC;IACDE,gBAAgB,CAACC,OAAO,EAAE;MACxB,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,kBAAkB,EAAEF,OAAO;MAChD,IAAI,CAACV,KAAI;IACX,CAAC;IACDa,cAAc,CAACC,KAAK,EAAE;MACpB,IAAI,CAACH,MAAM,CAACC,QAAQ,CAAC,gBAAgB